{
    "version": 2,
    "builds": [
      {
        "src": "./backend/server.js",  // path to your server.js file
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/build",    // path to your build folder
        "use": "@vercel/static"
      }
    ],
  

  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/backend/server.js"
      }
    ]
  }